{
  "name": "vue-tailwind-datepicker",
  "type": "module",
  "version": "1.7.4",
  "description": "A date range picker component for Vue.js 3 using Tailwind CSS",
  "author": {
    "name": "elreco",
    "email": "alexandre@heypongo.com",
    "url": "https://github.com/elreco"
  },
  "homepage": "https://vue-tailwind-datepicker.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/elreco/vue-tailwind-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/elreco/vue-tailwind-datepicker/issues",
    "email": "alexandre@heypongo.com"
  },
  "keywords": [
    "vue-tailwind-datepicker",
    "vue-datepicker",
    "vue-daterangepicker",
    "vue-daterange-picker",
    "tailwind-datepicker",
    "tailwind-daterange-picker"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-tailwind-datepicker.js",
      "require": "./dist/vue-tailwind-datepicker.umd.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/vue-tailwind-datepicker.umd.cjs",
  "module": "./dist/vue-tailwind-datepicker.js",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "contributors": [],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "typecheck": "vue-tsc --noEmit",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": ":sparkles: Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": ":bug: Fixes",
                "hidden": false
              },
              {
                "type": "docs",
                "section": ":memo: Documentation",
                "hidden": false
              },
              {
                "type": "style",
                "section": ":barber: Code-style",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": ":zap: Refactor",
                "hidden": false
              },
              {
                "type": "perf",
                "section": ":fast_forward: Performance",
                "hidden": false
              },
              {
                "type": "test",
                "section": ":white_check_mark: Tests",
                "hidden": false
              },
              {
                "type": "ci",
                "section": ":repeat: CI",
                "hidden": false
              },
              {
                "type": "chore",
                "section": ":repeat: Chore",
                "hidden": false
              }
            ]
          }
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ]
    ]
  },
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "tailwindcss": "^4.1.11",
    "vue": "^3.3.13"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@headlessui/vue": "1.7.23",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^12.0.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.8.10",
    "@vitejs/plugin-vue": "^4.4.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "esbuild": "^0.19.12",
    "eslint": "^8.51.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.49",
    "postcss-prefix-selector": "^1.16.1",
    "postcss-preset-env": "^9.2.0",
    "semantic-release": "^22.0.12",
    "tailwindcss": "^4.1.11",
    "vite": "^4.4.12",
    "vite-plugin-dts": "3.6.4",
    "vitepress": "^1.0.0-rc.45",
    "vue": "^3.3.13",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
